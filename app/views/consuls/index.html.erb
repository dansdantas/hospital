<h1>Listing consuls</h1>

<table>
  <tr>
    <th>Data</th>
    <th>Hora</th>
    <th>Paciente</th>
    <th>Medico</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @consuls.each do |consul| %>
  <tr>
    <td><%= consul.data %></td>
    <td><%= consul.hora %></td>
    <td><%= consul.paciente.nome %></td>
    <td><%= consul.medico.nome %></td>
    <td><%= link_to 'Show', consul %></td>
    <td><%= link_to 'Edit', edit_consul_path(consul) %></td>
    <td><%= link_to 'Excluir', consul, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Consul', new_consul_path %>
