<h1>Listing pacientes</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Cpf</th>
    <th>Sexo</th>
    <th>Email</th>
    <th>Telefone</th>
    <th>Data</th>
    <th>Logradouro</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pacientes.each do |paciente| %>
  <tr>
    <td><%= paciente.nome %></td>
    <td><%= paciente.cpf %></td>
    <td><%= paciente.sexo %></td>
    <td><%= paciente.email %></td>
    <td><%= paciente.telefone %></td>
    <td><%= paciente.data %></td>
    <td><%= paciente.logradouro %></td>
    <td><%= link_to 'Show', paciente %></td>
    <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
    <td><%= link_to 'Excluir', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Paciente', new_paciente_path %>
